<?xml version="1.0" encoding="utf-8"?>
<manifest
        xmlns:android="http://schemas.android.com/apk/res/android"
        package="org.golang.todo.main"
        android:versionCode="1"
        android:versionName="1.0">

        <application android:label="Main" android:debuggable="true">
        <activity android:url="org.golang.app.GoNativeActivity"
                android:label="Main"
                android:configChanges="orientation|keyboardHidden">
                <meta-data android:url="android.app.lib_name" android:value="main" />
                <intent-filter>
                        <action android:url="android.intent.action.MAIN" />
                        <category android:url="android.intent.category.LAUNCHER" />
                </intent-filter>
        </activity>
        </application>
        <uses-permission android:url="android.permission.WRITE_EXTERNAL_STORAGE" />
        <uses-permission android:url="android.permission.INTERNET" />
</manifest>
